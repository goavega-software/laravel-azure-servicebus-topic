{
    "name": "goavega-software/laravel-azure-servicebus",
    "description": "Extending the Laravel queue to support the Microsoft Azure Service Bus Queues & topics.",
    "version": "7.0.0",
    "keywords": [
        "laravel",
        "azure",
        "servicebus",
        "queues"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Goavega Team",
        "email": "opensource@goavega.com"
    }],
    "require": {
        "php": ">=7.1.10",
        "illuminate/container": ">=7.0.0 <8.0.0",
        "illuminate/support": ">=7.0.0 <8.0.0",
        "illuminate/encryption": ">=7.0.0 <8.0.0",
        "illuminate/queue": ">=7.0.0 <8.0.0",
        "microsoft/windowsazure": "0.5.7"
    },
    "autoload": {
        "psr-4": {
            "Goavega\\LaravelAzureServicebus\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Goavega\\LaravelAzureServicebus\\Support\\ServiceProvider"
            ]
        }
    }
}